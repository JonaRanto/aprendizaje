{
  "name": "control-vacaciones",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:dev": "ng serve --c=development",
    "start:prod": "ng serve --c=production",
    "build:dev": "ng build --c=development",
    "build:prod": "ng build --c=production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "backend": "json-server --watch ./src/app/data/mocks/db.json",
    "backend:delay": "json-server --delay 1500 --watch ./src/app/data/mocks/db.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick && npm run lint",
      "pre-push": ""
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.14",
    "@angular/common": "~11.2.14",
    "@angular/compiler": "~11.2.14",
    "@angular/core": "~11.2.14",
    "@angular/forms": "~11.2.14",
    "@angular/platform-browser": "~11.2.14",
    "@angular/platform-browser-dynamic": "~11.2.14",
    "@angular/router": "~11.2.14",
    "bootstrap": "^5.1.3",
    "json-server": "^0.17.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.13",
    "@angular/cli": "~11.2.14",
    "@angular/compiler-cli": "~11.2.14",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "husky": "^7.0.4",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.5.0",
    "lint": "^0.7.0",
    "pretty-quick": "^3.1.3",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  }
}
